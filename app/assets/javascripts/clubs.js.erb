$(function(){
  $('button.accept').click(AcceptRequest);
  $('button.decline').click(DeclineRequest);
});

function AcceptRequest() {
  var id = this.id;
  var row = $(this).parent();
  $.ajax({
    type: 'PUT',
    url: ('/votes/' + id),
    data: { value: true },
    complete: function() {
      row.hide();
    }
  });
